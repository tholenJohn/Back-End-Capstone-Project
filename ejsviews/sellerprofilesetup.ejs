<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <% include ../partials/header %>
  <title>Serinde</title>
</head>

<body>
    <div class="jumbotron">
  <form action="/sellerprofilesetup" method="POST">
    <h1>Set up your Seller Profile</h1>
    <div class="form-group">
      <label for="first">Company Name</label>
      <input type="text" class="form-control" id="cname" name="cname" value="Individual">
    </div>
    <div class="form-group">
      <label for="last">Company Location</label>
      <input type="text" class="form-control" id="clocation" name="clocation" placeholder="Enter company location">
    </div>
    <button type="submit" class="btn btn-primary">Start Selling!</button>
  </form>
  <br>
  <a href="/" class="btn btn-danger">Back to Home</a>
  </div>
  <% include ../partials/scripts %>
</body>

</html>